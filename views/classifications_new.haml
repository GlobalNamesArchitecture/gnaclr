%h1 Upload classification

%form{ :action => "/classifications", :method => :post,  :enctype => "multipart/form-data" }
  %fieldset
    %legend Classification
    .form_field
      %input{ :type => "text", :name => "agent", :id => "agent" }
      %label{ :for => "agent" } Author
    .form_field
      %input{ :type => "text", :name => "name", :id => "name" }
      %label{ :for => "name" } Classification Name
    .form_field
      %input{ :type => "text", :name => "uuid", :id => "uuid" }
      %label{ :for => "uuid" } UUID
    .form_field
      %input{ :type => "file", :accept => "application/x-gzip;application/zip", :id => "file", :name => "file" }
    .form_field
      %input{ :type => "submit", :name => "submit", :value => "Upload" }
        

